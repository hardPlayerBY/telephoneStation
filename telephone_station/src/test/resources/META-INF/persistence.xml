<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">
	<persistence-unit name="station" transaction-type="RESOURCE_LOCAL">
		<class>com.telephone_station.datamodel.Abonent</class>
		<class>com.telephone_station.datamodel.Count</class>
		<class>com.telephone_station.datamodel.Service</class>
		<class>com.telephone_station.datamodel.Admin</class>
		<class>com.telephone_station.datamodel.User</class>
		<properties>
			<property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver" />
			<property name="hibernate.connection.url" value="jdbc:mysql://localhost/station" />
			<property name="hibernate.connection.username" value="biba" />
			<property name="hibernate.connection.password" value="biba" />
		</properties>
	</persistence-unit>
</persistence>